<launch>
	<arg name="map"                            default="test_4x4.jpg"/>
	<arg name="physics_frequency"              default="100.0"/>
	<arg name="physics_step_size"              default="0.01"/>
	<arg name="render_frequency"               default="30.0"/>
	<arg name="laser_max_angle"                default="0.79"/>
	<arg name="laser_max_dist"                 default="10.0"/>
	<arg name="robot_diam"                     default="0.8"/>
	<arg name="publish_observations_frequency" default="10"/>
	<arg name="laser_nrays"                    default="10"/>
	<arg name="velocity_iterations"            default="6"/>
	<arg name="position_iterations"            default="2"/>
	<arg name="render_height"                  default="700"/>
	<arg name="draw_laser"                     default="false"/>
	<arg name="number_of_agents"               default="1"/>

	<include file="$(find mapf_environment)/launch/environment.launch">
		<arg name="map"                     value="$(arg map)"/>
		<arg name="physics_frequency"       value="$(arg physics_frequency)"/>
		<arg name="physics_step_size"       value="$(arg physics_step_size)"/>
		<arg name="render_frequency"        value="$(arg render_frequency)"/>
		<arg name="laser_max_angle"         value="$(arg laser_max_angle)"/>
		<arg name="laser_max_dist"          value="$(arg laser_max_dist)"/>
		<arg name="robot_diam"              value="$(arg robot_diam)"/>
		<arg name="publish_observations_frequency" value="$(arg publish_observations_frequency)"/>
		<arg name="laser_nrays"             value="$(arg laser_nrays)"/>
		<arg name="velocity_iterations"     value="$(arg velocity_iterations)"/>
		<arg name="position_iterations"     value="$(arg velocity_iterations)"/>
		<arg name="render_height"           value="$(arg render_height)"/>
		<arg name="draw_laser"              value="$(arg draw_laser)"/>
	</include>

	<include file="$(find mapf_maddpg_agent)/launch/launch_2_agents.launch"/>
</launch>
